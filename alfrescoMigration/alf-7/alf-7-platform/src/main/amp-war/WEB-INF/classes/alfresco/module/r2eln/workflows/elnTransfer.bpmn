<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:activiti="http://activiti.org/bpmn" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:omgdc="http://www.omg.org/spec/DD/20100524/DC" xmlns:omgdi="http://www.omg.org/spec/DD/20100524/DI" typeLanguage="http://www.w3.org/2001/XMLSchema" expressionLanguage="http://www.w3.org/1999/XPath" targetNamespace="http://www.activiti.org/test">
  <process id="r2elnTransfer" name="R2ELN Transfer Process" isExecutable="true">
    <startEvent id="start" activiti:formKey="elnwf:submitReviewTask">
      <extensionElements>
        <activiti:executionListener event="complete" class="org.alfresco.repo.workflow.activiti.tasklistener.ScriptTaskListener">
          <activiti:field name="script">
            <activiti:string><![CDATA[execution.setVariable('elnwf_workflowType', task.getVariable('elnwf_workflowType'));
logger.log('elnwf_workflowType'+ task.getVariable('elnwf_workflowType'));]]></activiti:string>
          </activiti:field>
        </activiti:executionListener>
      </extensionElements>
    </startEvent>
    <userTask id="reviewTask" name="Review Task" activiti:assignee="${elnwf_assignee.properties.userName}" activiti:formKey="wf:activitiReviewTask">
      <extensionElements>
        <activiti:taskListener event="complete" class="org.alfresco.repo.workflow.activiti.tasklistener.ScriptTaskListener">
          <activiti:field name="script">
            <activiti:string><![CDATA[execution.setVariable('wf_reviewOutcome', task.getVariable('wf_reviewOutcome'));
execution.setVariable('elnwf_contentId', task.getVariable('elnwf_contentId'));]]></activiti:string>
          </activiti:field>
        </activiti:taskListener>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow2" sourceRef="reviewTask" targetRef="reviewDecision"></sequenceFlow>
    <exclusiveGateway id="reviewDecision" name="Review Decision"></exclusiveGateway>
    <sequenceFlow id="flow3" sourceRef="reviewDecision" targetRef="approveTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${wf_reviewOutcome == 'Approve'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow4" sourceRef="reviewDecision" targetRef="rejectedTask"></sequenceFlow>
    <userTask id="rejectedTask" name="Document Rejected" activiti:assignee="${initiator.exists() ? initiator.properties.userName : 'admin'}" activiti:formKey="elnwf:rejectedTask">
      <documentation>The document was reviewed and rejected.</documentation>
      <extensionElements>
        <activiti:taskListener event="complete" class="org.alfresco.repo.workflow.activiti.tasklistener.ScriptTaskListener">
          <activiti:field name="script">
            <activiti:string><![CDATA[execution.setVariable('bpm_outcome', task.getVariable('bpm_outcome'));]]></activiti:string>
          </activiti:field>
        </activiti:taskListener>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow6" sourceRef="exclusivegateway2" targetRef="usertask1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${bpm_outcome == 'Resubmit'}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="end"></endEvent>
    <serviceTask id="approveTask" name="Document Approved" activiti:async="true" activiti:class="com.argo.r2eln.wf.delegate.ApprovedDelegate"></serviceTask>
    <sequenceFlow id="flow7" sourceRef="approveTask" targetRef="end"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow10" sourceRef="rejectedTask" targetRef="exclusivegateway2">
      <extensionElements>
        <activiti:executionListener event="take" class="org.alfresco.repo.workflow.activiti.listener.ScriptExecutionListener">
          <activiti:field name="script">
            <activiti:string><![CDATA[logger.log(bpm_outcome);]]></activiti:string>
          </activiti:field>
        </activiti:executionListener>
      </extensionElements>
    </sequenceFlow>
    <sequenceFlow id="flow11" sourceRef="exclusivegateway2" targetRef="end">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${bpm_outcome == 'Cancel'}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="usertask1" name="ConfirmTask" activiti:assignee="${elnwf_receiver.exists() ? elnwf_receiver.properties.userName : 'admin'}" activiti:formKey="elnwf:confirmTask">
      <extensionElements>
        <activiti:taskListener event="complete" class="org.alfresco.repo.workflow.activiti.tasklistener.ScriptTaskListener">
          <activiti:field name="script">
            <activiti:string><![CDATA[execution.setVariable('bpm_outcome', task.getVariable('bpm_outcome'));
execution.setVariable('elnwf_contentId', task.getVariable('elnwf_contentId'));
execution.setVariable('elnwf_receiver', task.getVariable('elnwf_receiver'));
execution.setVariable('elnwf_assignee', task.getVariable('elnwf_assignee'));]]></activiti:string>
          </activiti:field>
        </activiti:taskListener>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow12" sourceRef="usertask1" targetRef="exclusivegateway3">
      <extensionElements>
        <activiti:executionListener event="start" class="org.alfresco.repo.workflow.activiti.listener.ScriptExecutionListener">
          <activiti:field name="script">
            <activiti:string><![CDATA[logger.log('bpm_outcome: '+bpm_outcome);]]></activiti:string>
          </activiti:field>
        </activiti:executionListener>
      </extensionElements>
    </sequenceFlow>
    <exclusiveGateway id="exclusivegateway3" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow14" sourceRef="exclusivegateway3" targetRef="rejectedTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${bpm_outcome == 'Reject'}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="mailTask" name="Mail Task" activiti:class="com.argo.r2eln.wf.delegate.SendMailDelegate"></serviceTask>
    <sequenceFlow id="flow17" sourceRef="exclusivegateway3" targetRef="mailTask"></sequenceFlow>
    <sequenceFlow id="flow18" sourceRef="mailTask" targetRef="reviewTask"></sequenceFlow>
    <sequenceFlow id="flow19" sourceRef="start" targetRef="usertask1"></sequenceFlow>
  </process>
