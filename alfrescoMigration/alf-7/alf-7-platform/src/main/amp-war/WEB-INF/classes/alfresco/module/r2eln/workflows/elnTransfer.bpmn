      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="mailTask" id="BPMNShape_mailTask">
        <omgdc:Bounds height="55.0" width="105.0" x="370.0" y="188.0"></omgdc:Bounds>
